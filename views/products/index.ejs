<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>
<main>
<section>
    <h2><%= title %></h2>
    <h4>Add your favorite beauty products here</h4>

    <ul>
    <div class="parent-product"></div>
        <% products.forEach(product => { %>

            <li>
                <a href="/products/<%= product._id %>"><%= product.name %></a>
                 is a <%= product.clean ? 'clean' : 'not clean' %> product
             </li>   
            
        <% }) %>
    </div>
</ul>
</section>
</main>

<%- include('../partials/footer') %>